    "wifi",
    "cli",
    "mac",
    "wireless",
    "network"
  ],
  "license": "MIT",
  "homepage": "https://github.com/danyshaanan/osx-wifi-cli",
  "repository": "danyshaanan/osx-wifi-cli",
